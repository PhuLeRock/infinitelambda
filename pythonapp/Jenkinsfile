pipeline {
    agent any
    environment {
        AWS_DEFAULT_REGION = "ap-southeast-1"
        AWS_DEFAULT_OUTPUT = "text"
    }

    stages {
        stage("git checkout") {
            steps {
                git credentialsId: 'phulegit', url: 'https://github.com/PhuLeRock/infinitelambda.git'
            }
        }           
    
        stage('push test') {
            steps {
                script {
                    docker.withRegistry('https://667656621301.dkr.ecr.ap-southeast-1.amazonaws.com/ecr001', 'ecr:ap-southeast-1:phule') {
                    docker.image("pythonapp").push(1.0)
                    }
                }
                }
            }
    }    
}            